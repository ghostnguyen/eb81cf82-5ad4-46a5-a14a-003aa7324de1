@model IEnumerable<OAMS.Models.SiteMonitoring>
@{
    ViewBag.Title = "Monitoring Detail Report";
    Layout = "~/Views/Shared/SiteLayoutRpt.cshtml";
}
@foreach (var item in Model)
{
    if ((bool)ViewBag.Old)
    {
    @(Html.Partial("~/Views/SiteMonitoring/DisplayTemplates/ViewDetail.ascx", item))
    }
    else
    {
    @(Html.Partial("~/Views/SiteMonitoring/DisplayTemplates/ViewDetail1.ascx", item)) 
    }

}
