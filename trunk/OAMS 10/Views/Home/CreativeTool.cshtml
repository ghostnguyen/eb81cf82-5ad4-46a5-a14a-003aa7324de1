<script type="text/javascript">
    function JScallSetBackground(url, p1x, p1y, p2x, p2y, p3x, p3y, p4x, p4y) {
        var obj = swfobject.getObjectById("myCom");
        if (obj && typeof obj.JScallSetBackground != "undefined") {
            obj.JScallSetBackground(url, p1x, p1y, p2x, p2y, p3x, p3y, p4x, p4y);
        }
    }
    function JScallSetReplaceImage(url) {
        var obj = swfobject.getObjectById("myCom");
        if (obj && typeof obj.JScallSetReplaceImage != "undefined") {
            obj.JScallSetReplaceImage(url);
        }
    }

    function JScallGetValue() {
        var obj = swfobject.getObjectById("myCom");
        if (obj && typeof obj.JScallGetValue != "undefined") {
            obj.JScallGetValue();
        }
    }

    function returnValue(p1x, p1y, p2x, p2y, p3x, p3y, p4x, p4y) {
        //alert( p1x.toString().indexOf("NaN") );
        if (p1x.toString() == "NaN")
            alert("Not data");
        else
            alert("data received \np1 (" + p1x + ", " + p1y + ")  \np2 (" + p2x + ", " + p2y + ")  \np3 (" + p3x + ", " + p3y + ")  \np4 (" + p4x + ", " + p4y + ")");
    }

